{
  "name": "postme",
  "version": "1.0.0",
  "description": "This is a reference app for node backend",
  "main": "src/server/index.js",
  "author": "Teddy Odhiambo <tedb19@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon src/server/index.js --ignore test/ --ignore './logs*.json' --watch src/* --exec babel-node",
    "test": "lab -T node_modules/lab-babel -t 70 -S -I __core-js_shared__ -v -C",
    "test-coverage": "lab -c -a code -r html -o test/coverage/coverage.html -T node_modules/lab-babel -I __core-js_shared__"
  },
  "dependencies": {
    "glue": "5.0.0",
    "hapi": "17.5.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "blipp": "3.0.0",
    "code": "5.2.0",
    "eslint": "4.19.1",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "3.1.0",
    "inert": "5.1.0",
    "lab": "15.4.5",
    "lab-babel": "1.1.1",
    "lout": "11.0.1",
    "nodemon": "1.17.5",
    "vision": "5.3.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
